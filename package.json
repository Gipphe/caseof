{
	"name": "caseof",
	"description": "Elm and Haskell-inspired case-of",
	"keywords": [
		"pattern-matching",
		"pattern",
		"matching",
		"case",
		"expression",
		"case-expression",
		"case-of"
	],
	"version": "5.0.0",
	"license": "MIT",
	"author": "Victor Nascimento Bakke",
	"main": "dist/index.js",
	"homepage": "https://github.com/Gipphe/caseof",
	"repository": {
		"type": "git",
		"url": "https://github.com/Gipphe/caseof.git"
	},
	"files": [
		"index.js"
	],
	"scripts": {
		"build": "tsc",
		"generateDocs": "transcribe --url 'https://github.com/Gipphe/caseof/blob/master/{filename}#L{line}' --insert-into README.md -- index.ts",
		"lint": "eslint --ext ts,js src test",
		"fmt": "prettier --write .",
		"test": "jest",
		"coverage": "jest --coverage",
		"coverage:post": "npm run coverage && cat ./coverage/lcov.info | coveralls",
		"release": "npm run lint && npm run test && standard-version"
	},
	"devDependencies": {
		"@types/jest": "^27.5.1",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^5.25.0",
		"coveralls": "^3.1.1",
		"eslint": "8.15.0",
		"jest": "^28.1.0",
		"nyc": "^15.1.0",
		"prettier": "^2.6.2",
		"standard-version": "^9.5.0",
		"transcribe": "^1.1.2",
		"ts-jest": "^28.0.2",
		"typescript": "^4.6.4"
	}
}
